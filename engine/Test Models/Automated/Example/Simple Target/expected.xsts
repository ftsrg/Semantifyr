type TimeUnit : {
    Seconds,
    Minutes,
    Hours
}

var __Mission__unit : TimeUnit
var __Mission__x : integer = 1
var __Mission__isZero : boolean = false

trans {
    __Mission__unit := Minutes;
    havoc __Mission__x;
    __Mission__isZero := (__Mission__x == 0);
}

init {
    __Mission__unit := Seconds;
}

env {}

prop {
    (__Mission__isZero == false)
}
