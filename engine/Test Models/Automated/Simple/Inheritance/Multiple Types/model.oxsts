package Test

type Base {
    var x : Integer

    tran {
        x := 0
    }
}

type ChildA : Base
type ChildB : ChildA
type ChildC : ChildB
type ChildD : ChildC
type Child : ChildD

type Holder {
    feature base : Child
    instance child :> base : Child

    tran {
        inline base.main()
    }
}

target Mission {
    instance holder : Holder

    init {
        assume (true)
    }

    tran {
        inline holder.main()
    }

    prop {
        true
    }
}
