package Test

type Base {
    var x : Integer

    virtual tran {
        x := 0
    }
}

type Child : Base {
    override tran {
        x := 10
    }
}

type Holder {
    feature base : Base
    containment child :> base : Child

    tran {
        inline base.main()
    }
}

target Mission {
    containment holder : Holder

    init {
        assume (true)
    }

    tran {
        inline holder.main()
    }

    prop {
        true
    }
}
