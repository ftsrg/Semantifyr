package Test

type Base {
    var x : Integer

    virtual tran {
        x := 0
    }
}

type ChildA : Base {

}

type ChildB : ChildA {

}

type ChildC : ChildB {
    override tran {
        x := 10
    }
}

type ChildD : ChildC {
    override tran {
        x := 20
    }
}

type Child : ChildD {
    override tran {
        x := 30
    }
}

type Holder {
    feature base : Base
    instance child :> base : Child

    tran {
        inline base.main()
    }
}

target Mission {
    instance holder : Holder

    init {
        assume (true)
    }

    tran {
        inline holder.main()
    }

    prop {
        true
    }
}
