package Test

type Base {
    var x : Integer

    tran {
        x := 0
    }
}

type Child : Base {
    tran {
        x := 10
    }
}

type Holder {
    feature base : Child
    instance child :> base : Child

    tran {
        inline base.main()
    }
}

target Mission {
    instance holder : Holder

    init {
        assume (true)
    }

    tran {
        inline holder.main()
    }

    prop {
        true
    }
}
