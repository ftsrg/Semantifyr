package Test

type Simple {
    var x : Integer
}

target Mission {
    containment a : Simple
    containment b : Simple

    init {
        a.x := 0
        b.x := 10
    }

    tran {
        a.x := a.x + 1
        b.x := b.x - 1
    }

    prop {
        a.x != b.x
    }
}
