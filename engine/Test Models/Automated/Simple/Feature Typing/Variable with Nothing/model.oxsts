package Test

type Atom {

}

type Holder {
    feature atom : Atom

    var current : atom[0..1]
}

target Mission {
    containment holder : Holder {
        containment a :> atom : Atom
        containment b :> atom : Atom
        containment c :> atom : Atom
    }

    init {
        holder.current := Nothing
    }

    tran {
        holder.current := holder.b
    }

    prop {
        holder.current != holder.b
    }
}
