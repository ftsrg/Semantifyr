package Test

type Atom

type Holder {
    feature atom : Atom

    var current : atom[0..1] := Nothing
    var currentTwo : atom[0..1] := Nothing
}

target Mission {
    containment holder : Holder {
        containment a :> atom : Atom
        containment b :> atom : Atom
        containment c :> atom : Atom
    }

    init {
        holder.current := holder.a
    }

    tran {
        holder.current := holder.b
    }

    prop {
        holder.current != holder.b
    }
}
