package Test

type Atom {

}

type Holder {
    feature atom : Atom

    var current : atom[0..1] := Nothing
}

target Mission {
    instance holder : Holder {
        instance a :> atom : Atom
        instance b :> atom : Atom
        instance c :> atom : Atom
    }

    init {
        holder.current := Nothing
    }

    tran {
        holder.current := holder.b
    }

    prop {
        holder.current != holder.b
    }
}
