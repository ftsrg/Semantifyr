package Test

target Mission {
    var y : Boolean
    var x : Integer

    init {
        y := false
    }

    tran {
        choice {
            assume (x == 10)
            y := true
        } or {
            assume (x == 10)
            x := 10
        } else {
            x := 0
        }
    }

    prop {
        y == false
    }
}
