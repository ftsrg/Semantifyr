package Test

type Callable {
    virtual tran {
        assume (true)
    }
}

type SimpleCallable : Callable {
    var x : Integer

    override tran {
        x := 0
    }
}

type Component {
    tran execute(callable: Callable) {
        inline callable.main()
    }
}

target Mission {
    instance component : Component
    instance callable : SimpleCallable

    init {
        assume (false)
    }

    tran {
        inline component.execute(callable)
    }

    prop {
        false
    }
}
