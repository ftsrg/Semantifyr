package Test

type Simple {
    var x : Integer

    init {
        x := 0
    }

    tran {
        x := x + 1
    }
}

target Mission {
    instance a : Simple
    instance b : Simple
    instance c : Simple

    init {
        inline a.init()
        inline b.init()
        inline c.init()
    }

    tran {
        inline a.main()
        inline b.main()
        inline c.main()
    }

    prop {
        a.x != b.x
    }
}
