package Test

type Simple {
    var x : Integer

    tran {
        x := x + 1
    }
}

type Composite {
    feature simples : Simple[0..*]

    tran {
        inline seq simples -> main
    }
}

target Mission {
    instance composite : Composite {
        instance a :> simples : Simple
        instance b :> simples : Simple
    }

    init {
        assume (false)
    }

    tran {
        inline composite.main()
    }

    prop {
        false
    }
}
