type VerifyMissionEnvironment {
    feature statechart := SimpleStatechart()

    init {
        inline statechart.init()
    }

    tran {
        havoc (statechart.control)
        inline statechart.execute()
    }

    prop {
        !statechart.Operation.isActive
    }
}

// compile Verify.oxsts VerifyMissionEnvironment -o verify.xsts
